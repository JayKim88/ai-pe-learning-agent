{
  "name": "ai-pe-learning-agent",
  "version": "2.0.0",
  "description": "TypeScript-first multi-agent system for AI Product Engineer learning",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/hello-claude.ts",
    "build": "tsc",
    "cli": "tsx src/cli.ts",
